{
  "name": "onPress",
  "productName": "onPress",
  "version": "0.1.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "debug": "set DEBUG=*,-babel&&electron-forge start",
    "lint": "eslint src",
    "comp": "export NODE_ENV=production&&electron-compile src",
    "comp:win": "set NODE_ENV=production&&electron-compile src",
    "comp:wind": "set DEBUG=*,-babel&&set NODE_ENV=production&&electron-compile src",
    "pack": "export NODE_ENV=production&&electron-forge package",
    "pack:win": "set NODE_ENV=production&&electron-forge package",
    "pack:wind": "set DEBUG=*,-babel&&set NODE_ENV=production&&electron-forge package",
    "make": "electron-forge make",
    "maked": "set DEBUG=*,-babel&&electron-forge make",
    "package": "electron-forge package"
  },
  "keywords": [],
  "author": "kimorkim",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "icon": "src/image/app",
        "asar": true,
        "win32metadata": {
          "CompanyName": "kimorkim",
          "FileDescription": "Markdown Editor",
          "OriginalFilename": "onPress.exe",
          "ProductName": "onPress",
          "InternalName": "onPress"
        }
      },
      "electronWinstallerConfig": {
        "name": "onPress",
        "exe": "onPress.exe",
        "authors": "kimorkim",
        "iconUrl": "https://raw.githubusercontent.com/kimorkim/onPress/master/src/image/app.ico",
        "setupIcon": "./src/image/app.ico",
        "loadingGif": "./src/image/onPress.gif"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "onPress"
      }
    }
  },
  "dependencies": {
    "babel-runtime": "6.23.0",
    "codemirror": "5.24.2",
    "electron-compile": "6.1.3",
    "electron-devtools-installer": "^2.1.0",
    "i18n": "0.8.3",
    "less": "2.7.2",
    "lodash": "4.17.4",
    "markdown-it": "8.3.0",
    "markdown-it-emoji": "1.3.0",
    "normalize.css": "5.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-split-pane": "^0.1.61"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-env": "1.1.8",
    "babel-preset-react": "6.23.0",
    "electron-compilers": "^5.6.0",
    "electron-prebuilt-compile": "1.4.15",
    "eslint": "3.16.1",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0"
  }
}
